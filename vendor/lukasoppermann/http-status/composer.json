{
    "name": "lukasoppermann/http-status",
    "description": "A minimal package for working with HTTP statuses.",
    "keywords": [
        "Httpstatus",
        "status codes"
    ],
    "homepage": "https://github.com/lukasoppermann/http-status",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Oppermann",
            "email": "lukas@vea.re",
            "homepage": "http://vea.re",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "league/csv": "^7.1",
        "fabpot/php-cs-fixer": "^1.10",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "autoload": {
        "psr-4": {
            "Lukasoppermann\\Httpstatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lukasoppermann\\Httpstatus\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit; vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
